{
  "name": "avsc-json-sample",
  "version": "1.0.0",
  "description": "A librairy to generate predicable JSON sample from an avsc type",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "install": "npm run build"
  },
  "keywords": [
    "avro",
    "avsc",
    "avsc-json-sample",
    "json",
    "sample"
  ],
  "author": "Delphin Aubin",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "26.0.21",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4"
  },
  "dependencies": {
    "avsc": "5.5.3",
    "typescript": "4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "roots": [
      "src"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
